{
  "id": "openai",
  "schemaVersion": "v1",
  "displayName": "OpenAI Platform",
  "vendor": "OpenAI",
  "homepage": "https://platform.openai.com/docs/api-reference",
  "license": "MIT",
  "requiresCore": "^2.0.0",
  "canonical": "OPENAI_API_KEY",
  "aliases": [
    {
      "name": "OPENAI_KEY",
      "confidence": "high",
      "legacy": true,
      "ambiguous": false
    },
    {
      "name": "OPEN_AI_API_KEY",
      "confidence": "medium",
      "legacy": false,
      "ambiguous": false
    },
    {
      "name": "OAI_API_KEY",
      "confidence": "low",
      "legacy": false,
      "ambiguous": true
    }
  ],
  "capabilities": [
    "text.generate",
    "text.embed",
    "image.generate", 
    "image.analyze",
    "audio.transcribe",
    "moderate.content",
    "streaming"
  ],
  "transport": "http",
  "http": {
    "baseUrl": "https://api.openai.com/v1",
    "auth": {
      "type": "bearer",
      "headerName": "Authorization",
      "prefix": "Bearer "
    },
    "headers": {
      "User-Agent": "RAGnos-Vault/2.0.0",
      "Content-Type": "application/json"
    },
    "operations": {
      "validate": {
        "method": "GET",
        "path": "/models",
        "timeout": 10000,
        "expectedStatus": [200]
      }
    },
    "rateLimits": {
      "requests": 60,
      "window": "1m"
    },
    "retryPolicy": {
      "attempts": 3,
      "backoff": "exponential",
      "delay": 1000
    }
  },
  "security": {
    "scopes": ["api:read"],
    "networkAllowlist": [
      "api.openai.com",
      "*.openai.com"
    ],
    "piiRules": {
      "maskApiKey": true,
      "logRedaction": ["apiKey", "authorization"]
    }
  },
  "fingerprints": {
    "endpoints": [
      "api.openai.com",
      "platform.openai.com"
    ],
    "libraries": [
      "openai",
      "@openai/openai",
      "openai-api"
    ],
    "envPatterns": [
      "OPENAI_*",
      "OAI_*"
    ],
    "filePatterns": [
      "openai.config.*",
      ".openai*"
    ]
  },
  "metadata": {
    "tags": ["ai", "llm", "text-generation", "embeddings"],
    "category": "ai",
    "maturity": "stable",
    "support": {
      "official": true,
      "community": true,
      "enterprise": true
    }
  }
}