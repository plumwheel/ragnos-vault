{
  "name": "ragnos-vault",
  "version": "1.0.0",
  "description": "RAGnos Vault - Enterprise secrets management platform forked from Infisical",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ragnos-labs/ragnos-vault.git"
  },
  "author": "RAGnos Labs <labs@ragnos.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ragnos-labs/ragnos-vault/issues"
  },
  "homepage": "https://vault.ragnos.io",
  "keywords": [
    "secrets",
    "vault",
    "security",
    "encryption",
    "enterprise"
  ],
  "scripts": {
    "prepare": "husky install",
    "bootstrap": "tsx scripts/vault-bootstrap.ts",
    "cli": "tsx scripts/vault-cli.ts",
    "test": "./scripts/test-all.sh",
    "integration-test": "tsx scripts/integration-test.ts",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx frontend backend scripts",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --fix frontend backend scripts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:backend && npm run build:frontend",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "demo": "tsx scripts/vault-cli.ts demo"
  },
  "lint-staged": {
    "./{frontend,backend,scripts}/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "@radix-ui/react-radio-group": "^1.1.3",
    "argon2": "^0.31.0",
    "axios": "^1.6.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "secrets.js-grempe": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
