    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.57.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@ragnos-vault/sdk": "workspace:*",
    "zod": "^3.22.0"
